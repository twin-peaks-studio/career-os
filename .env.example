# SerpAPI key for Google Jobs search (free tier: 100/month)
SERPAPI_KEY=your_serpapi_key_here

# Supabase PostgreSQL connection string
# Get it from: Supabase Dashboard → Database → Settings → Connection string (URI)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Supabase Auth (get from Supabase Dashboard → Settings → API Keys)
# These are safe to expose publicly — security comes from RLS and server-side auth
NEXT_PUBLIC_SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
